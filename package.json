{
  "name": "@lsnsh/weui-vue",
  "version": "0.1.0",
  "description": "A Vue.js component library based on WeUI.",
  "main": "lib/weui-vue.common.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "bootstrap": "npm i",
    "build:file": "node build/bin/build-entry.js",
    "build:examples": "npm run build:file && cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "clean": "rm -rf lib",
    "dev": "npm run build:file && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "dist": "npm run clean && npm run build:file && webpack --config build/webpack.common.js"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@vue/babel-preset-app": "^4.1.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.0.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^3.0.0",
    "vue": "2.6.11",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "2.6.11",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lsnsh/weui-vue.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lsnsh/weui-vue/issues"
  },
  "homepage": "https://github.com/Lsnsh/weui-vue#readme",
  "directories": {
    "lib": "lib"
  }
}
